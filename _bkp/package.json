{
  "name": "bootstrap",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/wethegit/bootstrap",
  "author": "Marlon Marcello <marlon.marcello@gmail.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "compress": "node build_scripts/compress_images.js",
    "build": "node build_scripts/build.js",
    "start": "npm run build; node build_scripts/start.js",
    "production": "rm -rf node_modules/ yarn.lock package-lock.json production/ build/ .cache/*; npm i --only=prod; npm run build production; mv build/ production/",
    "wtcdev": "npm run build wtcdev && aws s3 sync --delete --cache-control max-age=0 ./build/ s3://bootstrap.wethe.work/dev",
    "wtcpub": "npm run build wtcpub && aws s3 sync --delete --cache-control max-age=0 --exclude dev/* ./build/ s3://bootstrap.wethe.work"
  },
  "dependencies": {
    "@babel/core": "~7.10.2",
    "@babel/plugin-proposal-class-properties": "~7.10.1",
    "@babel/plugin-transform-runtime": "~7.10.1",
    "@babel/polyfill": "~7.10.1",
    "@babel/preset-env": "~7.10.2",
    "@babel/runtime": "~7.10.2",
    "babel-loader": "~8.1.0",
    "chalk": "~4.0.0",
    "clone-deep": "~4.0.1",
    "core-js": "~3.6.5",
    "del": "~5.1.0",
    "dotenv": "~8.2.0",
    "dotenv-expand": "~5.1.0",
    "eslint": "~7.1.0",
    "eslint-config-prettier": "~6.11.0",
    "eslint-loader": "~4.0.2",
    "eslint-plugin-prettier": "~3.1.3",
    "favicons": "~6.1.0",
    "fs-extra": "~9.0.0",
    "glob-fs": "~0.1.7",
    "js-yaml": "~3.14.0",
    "node-sass": "~4.14.1",
    "node-sass-asset-functions": "~0.1.0",
    "node-sass-package-importer": "~5.3.2",
    "prettier": "~2.0.5",
    "pug": "~3.0.0",
    "sitemap": "~6.1.5",
    "stylelint": "~13.5.0",
    "stylelint-config-prettier": "~8.0.1",
    "stylelint-formatter-pretty": "~2.1.0",
    "stylelint-order": "~4.1.0",
    "stylelint-prettier": "~1.1.2",
    "stylelint-scss": "~3.17.2",
    "webpack": "~4.43.0",
    "wtc-autoplay-video": "~2.0.3",
    "wtc-controller-viewports": "~2.0.4",
    "wtc-gallery-component": "~1.4.2",
    "wtc-modal-view": "~3.0.0",
    "wtc-utility-helpers": "~3.0.1"
  },
  "devDependencies": {
    "chokidar": "~3.4.0",
    "imagemin": "~7.0.1",
    "imagemin-gifsicle": "~7.0.0",
    "imagemin-mozjpeg": "~9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "~8.0.0",
    "live-server": "~1.2.1",
    "md5-file": "~5.0.0"
  }
